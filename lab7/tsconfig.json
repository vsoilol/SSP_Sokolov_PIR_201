{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": ["./src/core/*"],
      "@screens/*": ["src/screens/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@my-types/*": ["src/types/*"],
      "@hooks/*": ["src/hooks/*"],
      "@src/*": ["./src/*"]
    }
  }
}
